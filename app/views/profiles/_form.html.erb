<%= form_with(model: metadata_profile, url: account_profiles_path(@account)) do |form| %>
  <%= render "shared/error_messages", resource: form.object %>

  <div class="flex justify-end items-start">
    <%= yield :profile_form_actions %>
    <%= form.submit class: primary_btn_classes('px-5 py-3') %>
  </div>
<% end %>